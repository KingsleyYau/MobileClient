<UserControl x:Class="CustomControl.NormalTileBackImg"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="336">

    <Grid x:Name="LayoutRoot" Width="336" Height="336"  Background="{StaticResource PhoneAccentBrush}">
        <Canvas HorizontalAlignment="Left" Height="336" VerticalAlignment="Top" Width="336" Background="{StaticResource PhoneAccentBrush}"/>

    </Grid>
</UserControl>